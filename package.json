    "production": "babel-node server",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postinstall": "npm run build",
    "eslint": "eslint .",
    "build": "NODE_ENV=production webpack --config ./webpack.production.js --progress --profile --colors",
